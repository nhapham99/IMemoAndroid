<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".Presentation.Home.TabFragment.ImageFragment"
    android:layout_marginLeft="@dimen/length_10dp"
    android:layout_marginRight="@dimen/length_10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/length_300dp">

        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/memo_image_viewPager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/length_10dp"
            android:layout_marginRight="@dimen/length_10dp"/>

        <me.relex.circleindicator.CircleIndicator3
            android:id="@+id/circle_indicator"
            android:layout_width="match_parent"
            android:layout_height="@dimen/length_50dp"
            android:layout_alignParentBottom="true"
            android:background="@null"
            app:ci_drawable_unselected="@drawable/circle_indicator_selected"
            app:ci_drawable="@drawable/circle_indicator_selected"
            />
    </RelativeLayout>

</FrameLayout>